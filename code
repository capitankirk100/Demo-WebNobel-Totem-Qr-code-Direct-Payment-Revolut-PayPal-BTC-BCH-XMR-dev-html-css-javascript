});

let isKioskMode = false;
const nonPaymentItems = ['Free Service 1', 'Free Product 2', 'Redirect Item 3'];

function toggleKioskMode() {
  isKioskMode = !isKioskMode;
  const kioskModeButton = document.getElementById('kioskModeButton');
  kioskModeButton.classList.toggle('active', isKioskMode);
  updateDisplay();
}

function updateDisplay() {
  const cardGrid = document.getElementById('cardGrid');
  cardGrid.innerHTML = '';
  jsonData.forEach(item => {
    const card = createCard(item);
    cardGrid.appendChild(card);
  });
}

function createCard(item) {
  const card = document.createElement('div');
  card.className = isListLayout ? 'list-item' : 'card';
  const isLiked = likedItems.includes(item.Nome);
  
  if (isListLayout) {
    card.innerHTML = `
      <div class="list-item-content">
        <img src="${item.URLFoto}" alt="${item.Nome}">
        <div class="list-item-info">
          <h3>${item.Nome}</h3>
          <p>${item.Categoria}</p>
        </div>
        <button class="heart-button ${isLiked ? 'active' : ''}" onclick="toggleLike(event, '${item.Nome}')">
          ${isLiked ? '‚ù§Ô∏è' : 'ü§ç'}
        </button>
        <button class="open-button" onclick="handleItemClick('${item.URL}', '${item.Nome}')">
          ${isKioskMode ? 'Scan üßæ' : 'OPEN'}
        </button>
      </div>
    `;
  } else {
    card.innerHTML = `
      <a href="${item.URL}" onclick="handleItemClick('${item.URL}', '${item.Nome}'); return false;">
        <img src="${item.URLFoto}" alt="${item.Nome}">
        <h3>${item.Nome}</h3>
      </a>
      <div class="heart-container">
        <button class="heart-button ${isLiked ? 'active' : ''}" onclick="toggleLike(event, '${item.Nome}')">
          ${isLiked ? '‚ù§Ô∏è' : 'ü§ç'}
        </button>
      </div>
    `;
  }
  
  return card;
}

function handleItemClick(url, itemName) {
  if (isKioskMode) {
    openScanWindow(url, itemName);
  } else {
    window.open(url, '_blank');
  }
}

function openScanWindow(url, itemName) {
  const overlay = document.createElement('div');
  overlay.className = 'scan-overlay';
  
  const content = document.createElement('div');
  content.className = 'scan-content';
  
  const title = document.createElement('h2');
  title.textContent = nonPaymentItems.includes(itemName) ? 'Scan per reindirizzamento' : 'In Attesa del tuo pagamento';
  
  const qrCode = document.createElement('div');
  qrCode.id = 'qrCode';
  
  const closeButton = document.createElement('button');
  closeButton.textContent = '√ó';
  closeButton.className = 'close-button';
  closeButton.onclick = () => document.body.removeChild(overlay);
  
  content.appendChild(closeButton);
  content.appendChild(title);
  content.appendChild(qrCode);
  
  if (!nonPaymentItems.includes(itemName)) {
    const paymentMethods = ['PayPal', 'Revolut', 'BTC', 'BCH', 'XMR'];
    const paymentButtons = document.createElement('div');
    paymentButtons.className = 'payment-buttons';
    
    paymentMethods.forEach(method => {
      const button = document.createElement('button');
      button.textContent = method;
      button.onclick = () => updateQRCode(method, url);
      paymentButtons.appendChild(button);
    });
    
    const loadingSpinner = document.createElement('div');
    loadingSpinner.className = 'loading-spinner';
    
    content.appendChild(paymentButtons);
    content.appendChild(loadingSpinner);
  }
  
  overlay.appendChild(content);
  document.body.appendChild(overlay);
  
  updateQRCode(nonPaymentItems.includes(itemName) ? 'Redirect' : 'PayPal', url);
}

function updateQRCode(method, url) {
  const qrCode = document.getElementById('qrCode');
  qrCode.innerHTML = `<img src="https://api.qrserver.com/v1/create-qr-code/?size=200x200&data=${encodeURIComponent(method + ':' + url)}" alt="QR Code for ${method}">`;
}

// Aggiungi questo event listener dopo aver definito la funzione toggleKioskMode
document.getElementById('kioskModeButton').addEventListener('click', toggleKioskMode);

// Assicurati di chiamare updateDisplay() all'avvio dell'applicazione
updateDisplay();
    </script>
</body>
</html>
